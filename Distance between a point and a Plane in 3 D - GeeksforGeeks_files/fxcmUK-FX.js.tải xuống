var FXCM_DISCLAIMER_FX_DIV = "disclosure";

function fxcmDisplayDisclaimer() {
	if (document.getElementById(FXCM_DISCLAIMER_FX_DIV)) {
		var disclaimerFX = "Losses can exceed your deposited funds."; 

		$('#' + FXCM_DISCLAIMER_FX_DIV).text(disclaimerFX);
	} else {
    		setTimeout(fxcmDisplayDisclaimer, 15);
  	}

}